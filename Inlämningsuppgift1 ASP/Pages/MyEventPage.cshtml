@page
@model Inlämningsuppgift1_ASP.Pages.MyEventPageModel

@{
    ViewData["Title"] = "MyEventPage";
}

<h1>My Events</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Event
            </th>
            <th>
                Place
            </th>
            <th>
                Address
            </th>
            <th>
                Date
            </th>
            <th>
                Organizer
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>


@foreach (var item in Model.Attendee.Event) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Place)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Organizer)
            </td>
        </tr>
}
    </tbody>
</table>
